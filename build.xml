<?xml version="1.0" encoding="UTF-8"?>
<project name="UQAI18" default="all">
    <target name="clean">
        <delete dir="build"/>
        <delete file="assignment1.jar"/>
    </target>

    <target name="compile">
        <mkdir dir="build/classes"/>
        <javac srcdir="src" destdir="build/classes"/>
    </target>

    <target name="all" depends="compile">
        <jar destfile="./Tester.jar" basedir="build/classes">
            <manifest>
                <!-- Uncomment the line for the desired main class -->
                <attribute name="Main-Class" value="tester.Tester"/>
                <!--<attribute name="Main-Class" value="navigation.NavigationSolver"/>-->
            </manifest>
        </jar>
    </target>
</project>
